<configuration>
	<system.web>
	</system.web>
	<appSettings>
		<!--xml配置文件路径-->
		<add key="XmlConfigFilePath" value="CombinerConfig.xml"/>
		<!--缓存时长,单位小时-->
		<add key="CacheDuration" value="6"/>
		<!--是否追加缓存配置,1:追,0:不追加-->
		<add key="IsAppendCacheExtension" value="1"/>
		<add key="ChinaCacheUserName" value="cn100"/>
		<add key="ChinaCachePassword" value="api@CDN"/>
		<add key="LoginName" value="Admin"/>
		<add key="PassWord" value="123"/>
	</appSettings>
	<system.net>
		<settings>
			<httpWebRequest useUnsafeHeaderParsing="true"/>
		</settings>
	</system.net>
</configuration>
